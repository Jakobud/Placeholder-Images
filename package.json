{
  "name": "placeholder-images",
  "displayName": "Placeholder Images",
  "description": "",
  "version": "0.0.1",
  "publisher": "JakeWilson",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:placeholderImages.placeholderImage"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "placeholderImages.placeholderImage",
        "title": "Placeholder Image",
        "description": "Insert a placeholder image"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Placeholder Images",
      "properties": {
        "placeholderImages.quoteStyle": {
          "type": "string",
          "default": "single",
          "enum": [
            "single",
            "double"
          ],
          "description": "Quote style for HTML tags. Possible values are 'single' or 'double'"
        },
        "placeholderImages.maxRecentImages": {
          "type": "integer",
          "default": 10,
          "description": "The maxmium number of Recent Placeholder Image URLs to save"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "copy-paste": "^1.3.0",
    "lodash": "^4.17.2",
    "open": "0.0.5",
    "vscode-cache": "^0.3.0"
  }
}
